<Window x:Class="ClaseListas.Win"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Win" Height="300" Width="300">
    <Grid>
        <ListBox x:Name="lbxLista" HorizontalAlignment="Left" Height="203" Margin="170,46,0,0" VerticalAlignment="Top" Width="105"/>
        <TextBox x:Name="txtNombre" HorizontalAlignment="Left" Height="23" Margin="22,46,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtRanking" HorizontalAlignment="Left" Height="23" Margin="22,96,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Nombre" HorizontalAlignment="Left" Margin="22,15,0,0" VerticalAlignment="Top"/>
        <Label Content="Ranking" HorizontalAlignment="Left" Margin="22,70,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnButton" Content="Clic here" HorizontalAlignment="Left" Margin="67,141,0,0" VerticalAlignment="Top" Width="75" Click="ClicResivirDatos"/>

    </Grid>
</Window>
